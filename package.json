{
  "name": "@uniquedj95/caseplus",
  "version": "1.0.1-0",
  "description": "A typescript library for converting string cases",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "Strings",
    "Typescript",
    "Casing"
  ],
  "author": "Daniel Justin <danieljmuleke@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
